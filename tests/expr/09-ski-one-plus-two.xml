<?xml version="1.0" encoding="utf-8"?>
<let xmlns="http://www.cardina1.red/_ns/xslt-lambda-calculus">
	<bind>
		<var>S</var>
		<lambda>
			<param>x</param>
			<param>y</param>
			<param>z</param>
			<body>
				<apply>
					<var>x</var>
					<var>z</var>
					<apply>
						<var>y</var>
						<var>z</var>
					</apply>
				</apply>
			</body>
		</lambda>
	</bind>
	<bind>
		<var>K</var>
		<lambda>
			<param>x</param>
			<param>y</param>
			<body>
				<var>x</var>
			</body>
		</lambda>
	</bind>
	<bind>
		<var>I</var>
		<lambda>
			<param>x</param>
			<body>
				<var>x</var>
			</body>
		</lambda>
	</bind>
	<body>
		<apply>
			<apply>
				<!-- add = SI(K(S(S(KS)K))) -->
				<var>S</var>
				<var>I</var>
				<apply>
					<var>K</var>
					<apply>
						<var>S</var>
						<apply>
							<var>S</var>
							<apply>
								<var>K</var>
								<var>S</var>
							</apply>
							<var>K</var>
						</apply>
					</apply>
				</apply>
			</apply>
			<var>
				<!-- 1 = I -->
				I
			</var>
			<apply>
				<!-- 2 = S(S(KS)K)I -->
				<var>S</var>
				<apply>
					<var>S</var>
					<apply>
						<var>K</var>
						<var>S</var>
					</apply>
					<var>K</var>
				</apply>
				<var>I</var>
			</apply>
		</apply>
	</body>
</let>
